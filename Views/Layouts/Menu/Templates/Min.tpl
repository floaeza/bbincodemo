<!-- Contenido template -->

<div id='MenuContainer'>

    <div id='MenuBackgrounds'></div>

    <img id='MainLogo' src='[@MainLogo]'>

    <div id='MenuWeather'>
        <canvas id='WeatherIcon' width='50' height='50'></canvas>
        <div id='WeatherSummary'></div>

        <div id='TemperatureGroup'>
            <span id='WeatherFarenheit'></span><span>&deg; F/</span>
            <span id='WeatherCelsius'></span><span>&deg; C</span>
        </div>
    </div>

    <div id='MenuDate'></div>
    <div id='MenuHour'></div>

    <div id='MenuSelected'></div>
    <div id='MenuDescription'></div>

    <b id='MenuChevronLeft'><i class='fa fa-chevron-left'></i></b>
    <br>
    <b id='MenuChevronRight'><i class='fa fa-chevron-right'></i></b>

    <div id='MenuBar'>
        <div class='MenuList' style='display: none;'></div>
        <div class='MenuList'></div>
        <div class='MenuList MenuFocus'></div>
        <div class='MenuList'></div>
        <div class='MenuList' style='display: none;'></div>
    </div>

</div>